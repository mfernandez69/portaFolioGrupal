{{#each proyectoGrupales}}
<div class="portfolio-modal modal fade" id="portfolioModal{{id}}" tabindex="-1" aria-labelledby="portfolioModal{{id}}" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header border-0"><button class="btn-close" type="button" data-bs-dismiss="modal" aria-label="Close"></button></div>
            <div class="modal-body text-center pb-5">
                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-lg-8">
                            <h2 class="portfolio-modal-title text-secondary text-uppercase mb-0">{{nombre}}</h2>
                            <div class="divider-custom">
                                <div class="divider-custom-line"></div>
                                <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
                                <div class="divider-custom-line"></div>
                            </div>
                            <img class="img-fluid rounded mb-5" src="assets/img/portfolio/cabin.png" alt="{{nombre}}" />
                            <h6 class="mb-4">{{descripcion}}</h6>
                            <h4>Lenguajes Utilizados:</h4>
                            <ul class="list-inline">
                                {{#each lenguajesUtilizados}}
                                    <li class="list-inline-item">{{this}}</li>
                                {{/each}}
                            </ul>
                            
                            <h4>Enlace GitHub:</h4>
                            <ul class="list-unstyled">
                                {{#each enlacesGithub}}
                                    <li><a href="{{this}}" target="_blank">{{this}}</a></li>
                                {{/each}}
                            </ul>
                            
                            <h4>Colaboradores:</h4>
                            <ul class="list-unstyled">
                                {{#each colaboradores}}
                                    <li>{{this.nombre}}</li>
                                {{/each}}
                            </ul>
                            <button class="btn btn-primary" data-bs-dismiss="modal">
                                <i class="fas fa-xmark fa-fw"></i>
                                Close Window
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{/each}}